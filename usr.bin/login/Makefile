CFLAGS+=#-DKERBEROS
PROG=	login
SRCS=	klogin.c login.c
DPADD=	${LIBUTIL} #${LIBKRB} ${LIBDES}
LDADD=	-lutil #-lkrb -ldes
.ifndef EXPORTABLE_SYSTEM
DPADD+=	${LIBCRYPT}
LDADD+= -lcrypt
.endif

BINOWN=	root
BINMODE=4555

.include <bsd.prog.mk>
