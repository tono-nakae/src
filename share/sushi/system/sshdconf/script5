#!/bin/sh
# $NetBSD: script5,v 1.1 2001/08/02 05:41:19 garbled Exp $

VAR=`grep "^$1" /etc/sshd.conf | awk '{print $2}'`
if [ -z "$VAR" ]; then
	VAR=`grep "^$1" /usr/share/sushi/system/sshdconf/defaults | awk '{print $2}'`
fi
if [ -z "$VAR" ]; then
	for i in INFO QUIET FATAL ERROR VERBOSE DEBUG
	do
		echo "$i"
	done
else
	echo "$VAR"
	for i in INFO QUIET FATAL ERROR VERBOSE DEBUG
	do
		echo "$i" | grep -v "$VAR"
	done
fi
