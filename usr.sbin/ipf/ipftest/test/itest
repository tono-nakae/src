#!/bin/sh
#
#	$NetBSD: itest,v 1.6 2000/06/12 13:35:53 veego Exp $
#

echo "$1...";
TESTDIR=$2;
[ -z "$2" ] && TESTDIR=$PWD'/../../../../dist/ipf/test';
/bin/cp /dev/null results/$1
ipf -nvf $TESTDIR/regress/$1 2>/dev/null > results/$1
cmp $TESTDIR/expected/$1 results/$1
status=$?
if [ $status = 0 ] ; then
	touch $1
fi
exit $status
