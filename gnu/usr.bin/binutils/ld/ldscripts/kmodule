/*	$NetBSD: kmodule,v 1.1 2008/10/12 23:00:53 mrg Exp $	*/

SECTIONS
{
   /* Pre-loaded modules must have merged BSS and data. */
   .data 0 :
   {
     *(.data)
     *(.data.*)
     *(.bss)
     *(.bss.*)
     *(COMMON)
   }
   /* Pre-loaded modules do not need the following. */
   /DISCARD/ :
   {
     *(.comment)
   }
}
