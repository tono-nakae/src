NOMAN=

BINDIR=	/usr/libexec

CFLAGS+=-I${.CURDIR} -I${.CURDIR}/../common \
	-I${.CURDIR}/../arch -I${.CURDIR}/../arch/${MACHINE}
YFLAGS=

.if exists(${.CURDIR}/../common/obj)
LDADD+=	${.CURDIR}/../common/obj/libcc1.a
.else
LDADD+=	${.CURDIR}/../common/libcc1.a
.endif

LDADD+=	-lgnumalloc
DPADD+= /usr/lib/libgnumalloc.a

.include <bsd.prog.mk>
