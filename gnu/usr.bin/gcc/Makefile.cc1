NOMAN=

BINDIR=	/usr/libexec

COMMON!=cd $(.CURDIR)/../common; \
	 printf "xxx: .MAKE\n\t@echo \$${.OBJDIR}\n" | ${MAKE} -s -f-

CFLAGS+=-I. -I${.CURDIR} -I${.CURDIR}/../common -I${.CURDIR}/../arch \
	-I${.CURDIR}/../arch/$(MACHINE_ARCH) -I$(COMMON)

YFLAGS=

LIBCC1!=cd $(.CURDIR)/../common; \
	printf "xxx: .MAKE\n\t@echo \$${.OBJDIR}/libcc1.a\n" | ${MAKE} -s -f-

LDADD+=	${LIBCC1} -lgnumalloc
DPADD+=	${LIBCC1} ${LIBGNUMALLOC}

.include <bsd.prog.mk>
