PROG	sbin/umount

PROG	usr/bin/ftp
PROG	usr/bin/gzip	usr/bin/gzcat usr/bin/gunzip
PROG	usr/bin/sed

PROG	usr/mdec/installboot

PROG	usr/sbin/chown	usr/bin/chgrp
