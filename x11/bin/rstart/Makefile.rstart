.include <bsd.own.mk>

.PATH:		${X11SRCDIR.xc}/programs/rstart/${CFDIR}
FILESDIR=	${X11LIBDIR}/rstart/${CFDIR}

.for _F in ${CFFILES}
${_F}.sed: ${_F}
	${_MKTARGET_CREATE}
	rm -f ${.TARGET}
	sed ${CFSED} < ${.ALLSRC} > ${.TARGET}

FILES+=			${_F}.sed
CLEANFILES+=		${_F}.sed
FILESNAME_${_F}.sed=	${_F}
.endfor

realall: ${FILES}
