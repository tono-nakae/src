include "arch/macppc/conf/GENERIC"

# Enable the hooks used for initializing the ram-disk.
options 	MEMORY_DISK_HOOKS
options 	MEMORY_DISK_IS_ROOT	# Force root on ram-disk
options 	MINIROOTSIZE=4096	# 2 Megabytes
