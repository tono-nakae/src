all: acpi_wakecode.h

acpi_wakecode.h: acpi_wakecode.S
	cc -c acpi_wakecode.S
	objcopy -S -O binary acpi_wakecode.o acpi_wakecode.bin
	sh ./genwakecode.sh > acpi_wakecode.h
	rm acpi_wakecode.bin acpi_wakecode.o
