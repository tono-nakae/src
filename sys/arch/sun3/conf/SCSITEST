#
# new config file for SCSITEST
#

include "std.sun3"

options		"SUN3_50"
options		"SUN3_60"

makeoptions	DEBUG = "-g"
maxusers	4
options		TIMEZONE = 480, DST = 2, MAXFDESCS=2048

# Standard options
options		"COMPAT_43"
options		"TCP_COMPAT_42"

# Options for all SUN3 machines
options		SWAPPAGER, VNODEPAGER, DEVPAGER
options		KTRACE
options		INET
options 	FFS
options 	NFSSERVER
options 	NFSCLIENT

#sun3-specific options
options		COMPAT_SUNOS

# sun3 debug options
options 	CONTEXT_DEBUG
options 	VMFAULT_TRACE
options		EXEC_DEBUG

config		netbsd swap generic

le0 at obio? addr ? level 3
#zs0 at obio? addr  0x00000 level 3 
#zs1 at obio? addr  0x20000 level 3 

si0 at obio? addr ? level ?

scsibus* at scsi?
sd* at scsibus? target ? lun ?
st* at scsibus? target ? lun ?
cd* at scsibus? target ? lun ?
ch* at scsibus? target ? lun ?

idprom0 at obctl? addr ? size ?

prom0 at mainbus0

pseudo-device loop
