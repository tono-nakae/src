# standard NetBSD/evbarm for IQ80310 options

machine	evbarm arm

options		EXEC_ELF32
options 	EXEC_AOUT
options 	EXEC_SCRIPT
options		IQ80310
options 	NEWINTR

# To support easy transit to ../arch/arm/arm32
options		PROG32
options		ARM32

makeoptions	LOADADDRESS="0xA0200000"
#makeoptions	LINKENTRY="-e nwstart"
