# standard NetBSD/netwinder options

machine	evbarm arm

options 	EXEC_AOUT
options 	EXEC_SCRIPT
options		EXEC_ELF32

# To support easy transit to ../arch/arm/arm32
options		PROG32
options		ARM32

makeoptions	LOADADDRESS="0xa0100000"
makeoptions	LINKENTRY="-e 0x10000"
makeoptions	BOARDTYPE="integrator"
