#
# standard NetBSD/arm32 options

machine	arm32

options 	SWAPPAGER	# paging; REQUIRED
options 	VNODEPAGER	# mmap() of files
options 	DEVPAGER	# mmap() of devices

options 	UVM		# Use UVM instead of Mach VM.

options 	EXEC_AOUT
options 	EXEC_SCRIPT
