RUMPNETCOMP=	agr bridge net net80211 netbt netinet netmpls npf
RUMPNETCOMP+=	local shmif virtif

RUMPNETSOCKIN=	sockin

.for var in ${RUMPNETCOMP}
RUMPNETLIBS+=lib${var}
RUMPNETLDADD+=-lrumpnet_${var}
.endfor

.for var in ${RUMPNETSOCKIN}
RUMPNETLIBS+=lib${var}
.endfor
