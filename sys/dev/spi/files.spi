define	spibus { }

device	spi { slave }
attach	spi at spibus
file	dev/spi/spi.c			spi | spibus

#
# SPI client devices
#

# TI TMP121 digital temperature sensor
device	tmp121temp: sysmon_envsys
attach	tmp121temp at spi
file	dev/spi/tmp121.c		tmp121temp
