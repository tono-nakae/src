define	acpiwmibus { }

# ACPI WMI mapper
device	acpiwmi: acpiwmibus
attach	acpiwmi at acpinodebus
file	dev/acpi/wmi/wmi_acpi.c		acpiwmi

# Dell WMI mappings
device	wmidell: sysmon_power
attach	wmidell at acpiwmibus
file	dev/acpi/wmi/wmi_dell.c		wmidell
