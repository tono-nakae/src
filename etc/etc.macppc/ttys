console	"/usr/libexec/getty std.38400"	vt100	off secure
ttyE0	"/usr/libexec/getty std.9600"	vt100	on  secure
ttyZ0	"/usr/libexec/getty std.38400"	vt100	on  secure
ttyZ1	"/usr/libexec/getty std.38400"	vt100	off secure
