#!/bin/sh
#
# $NetBSD: xfs,v 1.3 2000/05/13 08:45:10 lukem Exp $
#

# PROVIDE: xfs
# REQUIRE: mountall

. /etc/rc.subr

name="xfs"
command="/usr/X11R6/bin/${name}"
command_args="& sleep 2"
required_files="/usr/X11R6/lib/X11/fs/config"
extra_commands="reload"

load_rc_config $name
run_rc_command "$1"
