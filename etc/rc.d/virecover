# PROVIDE: virecover
# REQUIRE: mountcritremote
# BEFORE:  DAEMON
#
# XXX: should require `mail'!

. /etc/rc.subr

name="virecover"
rcvar=$name
command="/usr/libexec/${name}"
stop_cmd=":"
reload_cmd=":"

load_rc_config $name
run_rc_command "$1"
