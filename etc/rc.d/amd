#!/bin/sh
#
# $NetBSD: amd,v 1.1.1.1 2000/03/10 11:53:25 lukem Exp $
#

# PROVIDE: amd
# REQUIRE: nfsiod portmap mountall ypbind

. /etc/rc.subr
. /etc/rc.conf

name="amd"
command="/usr/sbin/${name}"
command_args='-p -a '$amd_dir' `sed s/#.*$// <'$amd_master'` >/var/run/amd.pid'
required_files="$amd_master"
required_dirs="$amd_dir"
required_vars="portmap"

run_rc_command "$1"
