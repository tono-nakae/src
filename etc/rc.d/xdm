#!/bin/sh
#
# $NetBSD: xdm,v 1.6 2000/09/19 13:04:39 lukem Exp $
#

# PROVIDE: xdm
# REQUIRE: DAEMON LOGIN wscons
# KEYWORD: shutdown

. /etc/rc.subr

name="xdm"
rcvar=$name
command="/usr/X11R6/bin/${name}"
pidfile="/var/run/${name}.pid"
required_files="/usr/X11R6/lib/X11/xdm/xdm-config"
extra_commands="reload"

load_rc_config $name
run_rc_command "$1"
