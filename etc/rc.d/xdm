#!/bin/sh
#
# $NetBSD: xdm,v 1.2 2000/03/13 04:04:10 lukem Exp $
#

# PROVIDE: xdm
# REQUIRE: DAEMON LOGIN wscons

. /etc/rc.subr
. /etc/rc.conf

name="xdm"
command="/usr/X11R6/bin/${name}"
pidfile="/var/run/${name}.pid"
required_files="/usr/X11R6/lib/X11/xdm/xdm-config"

run_rc_command "$1" "reload"
