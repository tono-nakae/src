#!/bin/sh
#
#	$NetBSD: veriexec,v 1.3 2004/08/13 18:08:03 mycroft Exp $
#

# PROVIDE: veriexec
# REQUIRE: mountcritlocal
# BEFORE:  securelevel

$_rc_subr_loaded . /etc/rc.subr

name="veriexec"
rcvar=$name
start_cmd="veriexec_start"

veriexec_start()
{
	echo -n "Loading fingerprints..."
	/sbin/veriexecctl /etc/signatures
	echo " done."
}

load_rc_config $name
run_rc_command "$1"
