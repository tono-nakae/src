#!/bin/sh
#
#	$NetBSD: veriexec,v 1.6 2005/10/11 21:24:09 elad Exp $
#

# PROVIDE: veriexec
# REQUIRE: mountall
# BEFORE:  securelevel sysctl

$_rc_subr_loaded . /etc/rc.subr

name="veriexec"
rcvar=$name
start_cmd="veriexec_start"

veriexec_start()
{
	echo -n "Loading fingerprints..."
	/sbin/veriexecctl load /etc/signatures
	echo " done."
}

load_rc_config $name
run_rc_command "$1"
