#!/bin/sh
#
# $NetBSD: kerberos,v 1.3 2000/04/30 13:23:33 lukem Exp $
#

# PROVIDE: kerberos
# REQUIRE: DAEMON LOGIN

. /etc/rc.subr
. /etc/rc.conf

arg=$1

name="kerberos"
command="/usr/sbin/${name}"
command_args=">> /var/log/${name}.log &"

run_rc_command "$arg"


name="kadmind"
rcvar="kerberos"
command="/usr/sbin/${name}"
command_args="-n >> /var/log/${name}.log &"

run_rc_command "$arg"
