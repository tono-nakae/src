#!/bin/sh
#
# $NetBSD: cgd,v 1.3 2004/08/13 18:08:03 mycroft Exp $
#

# PROVIDE: disks

$_rc_subr_loaded . /etc/rc.subr

name="cgd"
rcvar=$name
start_cmd="cgd_start"
stop_cmd=":"

cgd_start()
{
	if [ -f /etc/cgd/cgd.conf ]; then
		echo "Configuring CGD devices."
		cgdconfig -C
	fi
}

load_rc_config $name
run_rc_command "$1"
