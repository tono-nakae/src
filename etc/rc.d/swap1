#!/bin/sh
#
# $NetBSD: swap1,v 1.1.1.1 2000/03/10 11:53:24 lukem Exp $
#

# PROVIDE: localswap
# REQUIRE: ccd

. /etc/rc.subr
. /etc/rc.conf

name="swap1"

#		Add all block-type swap devices; these might be necessary
#		during disk checks.
#
start_cmd='swapctl -A -t blk'

#		Remove all block-type swap devices
#
stop_cmd="swapctl -U -t blk"

run_rc_command "$1"
