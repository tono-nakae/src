#!/bin/sh
#
# $NetBSD: hostapd,v 1.1 2006/04/30 12:54:26 rpaulo Exp $
#

# PROVIDE: hostapd
# REQUIRE: mountall beforemountlkm

$_rc_subr_loaded . /etc/rc.subr

name="hostapd"
rcvar=$name
command="/usr/sbin/hostapd"
required_files="/etc/hostapd.conf"
#pidfile="/var/run/hostapd.pid"

load_rc_config $name
run_rc_command "$1"
