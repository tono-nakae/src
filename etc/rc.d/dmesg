#!/bin/sh
#
# $NetBSD: dmesg,v 1.2 2000/04/30 12:39:56 lukem Exp $
#

# PROVIDE: dmesg
# REQUIRE: systemfs

. /etc/rc.subr
. /etc/rc.conf

name="dmesg"
start_precmd="checkyesno dmesg"
start_cmd="do_dmesg"
stop_cmd=":"

do_dmesg()
{
	rm -f /var/run/dmesg.boot
	dmesg $dmesg_flags > /var/run/dmesg.boot
}

run_rc_command "$1"
