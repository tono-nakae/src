#!/bin/sh
#
# $NetBSD: racoon,v 1.4 2004/08/13 18:08:03 mycroft Exp $
#

# PROVIDE: ike
# REQUIRE: isdnd kdc ppp
# BEFORE:  SERVERS

$_rc_subr_loaded . /etc/rc.subr

name="racoon"
rcvar=$name
command="/usr/sbin/${name}"
pidfile="/var/run/${name}.pid"
required_files="/etc/racoon/racoon.conf"

load_rc_config $name
run_rc_command "$1"
