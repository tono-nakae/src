#!/bin/sh
#
# $NetBSD: named,v 1.1.1.1 2000/03/10 11:53:24 lukem Exp $
#

# PROVIDE: named
# REQUIRE: servers

. /etc/rc.subr
. /etc/rc.conf

name="named"
command="/usr/sbin/${name}"
command_flags="$1"
pidfile="/var/run/${name}.pid"

run_rc_command "$1" "reload dumpdb notrace qrylog querylog stats trace"
