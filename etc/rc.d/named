#!/bin/sh
#
# $NetBSD: named,v 1.3 2000/04/30 13:23:34 lukem Exp $
#

# PROVIDE: named
# REQUIRE: SERVERS

. /etc/rc.subr
. /etc/rc.conf

name="named"
command="/usr/sbin/${name}"
command_flags="$1"
pidfile="/var/run/${name}.pid"
extra_commands="reload dumpdb notrace qrylog querylog stats trace"

run_rc_command "$1"
