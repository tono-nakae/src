$at_traceon; }

    { $at_traceoff
$as_echo "$at_srcdir/t_atf_config.at:90: grep 'testval' unaffected"
echo t_atf_config.at:90 >"$at_check_line_file"

if test -n "$at_traceon"; then
  ( $at_traceon; grep 'testval' unaffected ) >"$at_stdout" 2>"$at_stder1"
  at_func_filter_trace $?
else
  ( :; grep 'testval' unaffected ) >"$at_stdout" 2>"$at_stderr"
fi
at_status=$?
at_failed=false
at_func_diff_devnull "$at_stderr" || at_failed=:
at_func_diff_devnull "$at_stdout" || at_failed=:
at_func_check_status 1 $at_status "$at_srcdir/t_atf_config.at:90"

$at_failed && at_func_log_failure
$at_traceon; }

done

  $at_traceoff
  $at_times_p && times >"$at_times_file"
) 5>&1 2>&1 | eval $at_tee_pipe
at_status=`cat "$at_status_file"`
