# 7. t_atf_config.at:30: atf-config: querying of build-time variables
at_setup_line='t_atf_config.at:30'
at_desc="atf-config: querying of build-time variables"
$at_quiet $as_echo_n "  7: $at_desc   "
at_xfail=no
echo "#                             -*- compilation -*-" >> "$at_group_log"
(
  $as_echo "7. t_atf_config.at:30: testing ..."
  $at_traceon



all_vars="atf_arch atf_confdir atf_libexecdir atf_machine atf_pkgdatadir \
          atf_shell atf_workdir"
all_vars_no=7

{ $at_traceoff
$as_echo "$at_srcdir/t_atf_config.at:38: atf-config"
echo t_atf_config.at:38 >"$at_check_line_file"

if test -n "$at_traceon"; then
  ( $at_traceon; atf-config ) >"$at_stdout" 2>"$at_stder1"
  at_func_filter_trace $?
else
  ( :; atf-config ) >"$at_stdout" 2>"$at_stderr"
fi
at_status=$?
at_failed=false
at_func_diff_devnull "$at_stderr" || at_failed=:
echo stdout:; tee stdout <"$at_stdout"
at_func_check_status 0 $at_status "$at_srcdir/t_atf_config.at:38"

$at_failed && at_func_log_failure
$at_traceon; }

{ $at_traceoff
$as_echo "$at_srcdir/t_atf_config.at:39: test $(wc -l stdout | awk '{ print $1 }') = \${all_vars_no}"
echo t_atf_config.at:39 >"$at_check_line_file"

if { echo 'Not enabling shell tracing (command contains a ${...} parameter expansion)'
   false; }; then
  ( $at_traceon; test $(wc -l stdout | awk '{ print $1 }') = ${all_vars_no} ) >"$at_stdout" 2>"$at_stder1"
  at_func_filter_trace $?
else
  ( :; test $(wc -l stdout | awk '{ print $1 }') = ${all_vars_no} ) >"$at_stdout" 2>"$at_stderr"
fi
at_status=$?
at_failed=false
at_func_diff_devnull "$at_stderr" || at_failed=:
at_func_diff_devnull "$at_stdout" || at_failed=:
at_func_check_status 0 $at_status "$at_srcdir/t_atf_config.at:39"

$at_failed && at_func_log_failure
$at_traceon; }

for v in ${all_vars}; do
