  ( $at_traceon; grep 'fail .*always fails' stdout ) >"$at_stdout" 2>"$at_stder1"
  at_func_filter_trace $?
else
  ( :; grep 'fail .*always fails' stdout ) >"$at_stdout" 2>"$at_stderr"
fi
at_status=$?
at_failed=false
at_func_diff_devnull "$at_stderr" || at_failed=:
echo stdout:; cat "$at_stdout"
at_func_check_status 0 $at_status "$at_srcdir/t_test_program_list.at:37"

$at_failed && at_func_log_failure
$at_traceon; }

{ $at_traceoff
$as_echo "$at_srcdir/t_test_program_list.at:38: grep 'skip .*always skipped' stdout"
echo t_test_program_list.at:38 >"$at_check_line_file"

if test -n "$at_traceon"; then
  ( $at_traceon; grep 'skip .*always skipped' stdout ) >"$at_stdout" 2>"$at_stder1"
  at_func_filter_trace $?
else
  ( :; grep 'skip .*always skipped' stdout ) >"$at_stdout" 2>"$at_stderr"
fi
at_status=$?
at_failed=false
at_func_diff_devnull "$at_stderr" || at_failed=:
echo stdout:; cat "$at_stdout"
at_func_check_status 0 $at_status "$at_srcdir/t_test_program_list.at:38"

$at_failed && at_func_log_failure
$at_traceon; }

{ $at_traceoff
$as_echo "$at_srcdir/t_test_program_list.at:39: grep 'default .*explicitly' stdout"
echo t_test_program_list.at:39 >"$at_check_line_file"

if test -n "$at_traceon"; then
  ( $at_traceon; grep 'default .*explicitly' stdout ) >"$at_stdout" 2>"$at_stder1"
  at_func_filter_trace $?
else
  ( :; grep 'default .*explicitly' stdout ) >"$at_stdout" 2>"$at_stderr"
fi
at_status=$?
at_failed=false
at_func_diff_devnull "$at_stderr" || at_failed=:
echo stdout:; cat "$at_stdout"
at_func_check_status 0 $at_status "$at_srcdir/t_test_program_list.at:39"

$at_failed && at_func_log_failure
$at_traceon; }


{ $at_traceoff
$as_echo "$at_srcdir/t_test_program_list.at:41: $({
    old=$(pwd)
    while test $(pwd) != '/' -a ! -e ./Makefile; do
        cd ..
    done
    topdir=$(pwd)
    cd ${old}
    echo ${topdir}
})/tests/bootstrap/h_\${h} \
                          -s $({
    old=$(pwd)
    while test $(pwd) != '/' -a ! -e ./Makefile; do
        cd ..
    done
    topdir=$(pwd)
    cd ${old}
    echo ${topdir}
})/tests/bootstrap -l pass"
echo t_test_program_list.at:41 >"$at_check_line_file"

if { echo 'Not enabling shell tracing (command contains a ${...} parameter expansion)'
   false; }; then
  ( $at_traceon; $({
    old=$(pwd)
    while test $(pwd) != '/' -a ! -e ./Makefile; do
        cd ..
    done
    topdir=$(pwd)
    cd ${old}
    echo ${topdir}
})/tests/bootstrap/h_${h} \
                          -s $({
    old=$(pwd)
    while test $(pwd) != '/' -a ! -e ./Makefile; do
        cd ..
    done
    topdir=$(pwd)
    cd ${old}
    echo ${topdir}
})/tests/bootstrap -l pass ) >"$at_stdout" 2>"$at_stder1"
  at_func_filter_trace $?
else
  ( :; $({
    old=$(pwd)
    while test $(pwd) != '/' -a ! -e ./Makefile; do
        cd ..
    done
    topdir=$(pwd)
    cd ${old}
    echo ${topdir}
})/tests/bootstrap/h_${h} \
                          -s $({
    old=$(pwd)
    while test $(pwd) != '/' -a ! -e ./Makefile; do
        cd ..
    done
    topdir=$(pwd)
    cd ${old}
    echo ${topdir}
})/tests/bootstrap -l pass ) >"$at_stdout" 2>"$at_stderr"
fi
at_status=$?
at_failed=false
at_func_diff_devnull "$at_stderr" || at_failed=:
echo stdout:; tee stdout <"$at_stdout"
at_func_check_status 0 $at_status "$at_srcdir/t_test_program_list.at:41"

$at_failed && at_func_log_failure
$at_traceon; }

{ $at_traceoff
$as_echo "$at_srcdir/t_test_program_list.at:42: test $(wc -l stdout | awk '{ print $1 }') = 1"
echo t_test_program_list.at:42 >"$at_check_line_file"

if test -n "$at_traceon"; then
  ( $at_traceon; test $(wc -l stdout | awk '{ print $1 }') = 1 ) >"$at_stdout" 2>"$at_stder1"
  at_func_filter_trace $?
else
  ( :; test $(wc -l stdout | awk '{ print $1 }') = 1 ) >"$at_stdout" 2>"$at_stderr"
fi
at_status=$?
at_failed=false
at_func_diff_devnull "$at_stderr" || at_failed=:
echo stdout:; cat "$at_stdout"
at_func_check_status 0 $at_status "$at_srcdir/t_test_program_list.at:42"

$at_failed && at_func_log_failure
$at_traceon; }

{ $at_traceoff
$as_echo "$at_srcdir/t_test_program_list.at:43: grep 'pass .*always passes' stdout"
echo t_test_program_list.at:43 >"$at_check_line_file"

if test -n "$at_traceon"; then
  ( $at_traceon; grep 'pass .*always passes' stdout ) >"$at_stdout" 2>"$at_stder1"
