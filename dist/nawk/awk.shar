-{
-	Cell *x, *y;
-	char *s, *t;
-	int i;
-	fa *pfa;
-	int (*mf)(fa *, const char *) = match, mode = 0;
-
-	if (n == MATCHFCN) {
-		mf = pmatch;
-		mode = 1;
-	}
-	x = execute(a[1]);	/* a[1] = target text */
-	s = getsval(x);
