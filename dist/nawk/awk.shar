-Node *concat(Node *np)
-{
-	switch (rtok) {
-	case CHAR: case DOT: case ALL: case CCL: case NCCL: case '$': case '(':
-		return (concat(op2(CAT, np, primary())));
-	}
-	return (np);
-}
