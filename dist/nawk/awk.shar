-Cell *execute(Node *u)	/* execute a node of the parse tree */
-{
-	Cell *(*proc)(Node **, int);
-	Cell *x;
-	Node *a;
-
-	if (u == NULL)
-		return(True);
-	for (a = u; ; a = a->nnext) {
-		curnode = a;
-		if (isvalue(a)) {
-			x = (Cell *) (a->narg[0]);
-			if (isfld(x) && !donefld)
-				fldbld();
-			else if (isrec(x) && !donerec)
-				recbld();
-			return(x);
-		}
-		if (notlegal(a->nobj))	/* probably a Cell* but too risky to print */
-			FATAL("illegal statement");
-		proc = proctab[a->nobj-FIRSTTOKEN];
-		x = (*proc)(a->narg, a->nobj);
-		if (isfld(x) && !donefld)
-			fldbld();
-		else if (isrec(x) && !donerec)
-			recbld();
-		if (isexpr(a))
-			return(x);
-		if (isjump(x))
-			return(x);
-		if (a->nnext == NULL)
-			return(x);
-		tempfree(x);
-	}
-}
