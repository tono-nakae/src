-			yylval.cp = setsymtab(buf, tostring(buf), atof(buf), CON|NUM, symtab);
-			/* should this also have STR set? */
-			RET(NUMBER);
-		}
-	
-		yylval.i = c;
-		switch (c) {
-		case '\n':	/* {EOL} */
