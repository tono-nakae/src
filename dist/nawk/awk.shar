-	if (buf == 0 && (buf = (char *) malloc(bufsize)) == NULL)
-		FATAL( "out of space in yylex" );
-	if (sc) {
-		sc = 0;
-		RET('}');
-	}
-	if (reg) {
-		reg = 0;
-		return regexpr();
-	}
-/* printf("top\n"); */
-	for (;;) {
-		c = gettok(&buf, &bufsize);
-/* printf("gettok [%s]\n", buf); */
-		if (c == 0)
-			return 0;
