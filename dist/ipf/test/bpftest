#!/bin/sh
name=$1

. ./ipflib.sh

test_init

echo "$name...";
input=`expr $name : 'bpf-\(.*\)'`
n=1
/bin/cp /dev/null results/$name
( while read rule; do
	set_core $name $n
	echo "$rule" | ../ipftest -Rbr - -i input/$input >> results/$name &
	back=$!
	wait $back
	test_end_leak $?
	n=`expr $n + 1`
	echo "--------" >> results/$name
done ) < regress/$name
check_results $name
exit $status
