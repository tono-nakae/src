lsock.o: $(TOP)/ipsend/lsock.c
	$(CC) $(CCARGS) -c $(TOP)/ipsend/lsock.c -o $@
slinux.o: $(TOP)/ipsend/slinux.c
	$(CC) $(CCARGS) -c $(TOP)/ipsend/slinux.c -o $@
larp.o: $(TOP)/ipsend/larp.c
